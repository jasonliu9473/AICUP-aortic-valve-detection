    "# I. Data Preparation"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8ae8c8e7",
   "metadata": {},
   "source": [
    "### Combine all of the extracted dataset into one path"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "5c1174c3",
   "metadata": {},
   "outputs": [],
   "source": [
    "import os, shutil\n",
    "\n",
    "# THIS PART BELOW CAN BE CHANGED\n",
    "START = 1\n",
    "END   = 51\n",
    "# END OF CHANGEABLE PART\n",
    "\n",
    "def find_patient_root(root):\n",
    "    \"\"\"往下找，直到找到含有 patientXXXX 的資料夾\"\"\"\n",
    "    for dirpath, dirnames, filenames in os.walk(root):\n",
    "        if any(d.startswith(\"patient\") for d in dirnames):\n",
    "            return dirpath\n",
    "    return root\n",
    "\n",
    "IMG_ROOT = find_patient_root(\"/mnt/data/aorta/training_image\")\n",
    "LBL_ROOT = find_patient_root(\"/mnt/data/aorta/training_label\")\n",
    "\n",
    "os.makedirs(\"./datasets/images\", exist_ok=True)\n",
    "os.makedirs(\"./datasets/labels\", exist_ok=True)\n",
    "\n",
    "for i in range(START, END):\n",
    "    patient = f\"patient{i:04d}\"\n",
    "    img_dir = os.path.join(IMG_ROOT, patient)\n",
    "    lbl_dir = os.path.join(LBL_ROOT, patient)\n",
    "    if not os.path.isdir(img_dir):\n",
    "        continue\n",
    "\n",
    "    for fname in os.listdir(img_dir):\n",
    "        if not fname.endswith(\".png\"):\n",
    "            continue\n",
    "\n",
    "        img_path = os.path.join(img_dir, fname)\n",
    "        base, _ = os.path.splitext(fname)\n",
    "        # Add pseudo-labeling for no-label data
    "        label_path = os.path.join(lbl_dir, base + \".txt\") if os.path.isdir(lbl_dir) else None\n",
    "        out_lbl_path = os.path.join(f\"./datasets/labels/\", base + \".txt\")\n",
    "\n",
    "        shutil.copy2(img_path, f\"./datasets/images/\")\n",
    "        if label_path and os.path.exists(label_path):\n",
    "            shutil.copy2(label_path, out_lbl_path)\n",
    "        else:\n",
    "            with open(out_lbl_path, \"w\") as f:\n",
    "                pass"
   ]
  },
