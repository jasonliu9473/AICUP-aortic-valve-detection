  {
   "cell_type": "markdown",
   "id": "06b2e026",
   "metadata": {},
   "source": [
    "## 2. Predicting using Filtered Dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "85f0f47a",
   "metadata": {},
   "outputs": [],
   "source": [
    "from ultralytics import YOLO\n",
    "import os\n",
    "\n",
    "# THIS PART BELOW CAN BE CHANGED\n",
    "FOLD_NUM        = 7\n",
    "IOU             = 0.5\n",
    "CONF            = 0.017\n",
    "CLASSIFIED_PATH = \"./test_results_kfold/positive\"\n",
    "# END OF CHANGEABLE PART\n",
    "\n",
    "for idx in range(FOLD_NUM):\n",
    "    \n",
    "    # THIS PART BELOW CAN BE CHANGED\n",
    "    TRAINED_MODEL_PATH = f\"/mnt/data/aorta/yolo12x_fold7_best/yolo12x_fold7_1_{idx}/weights/best.pt\"\n",
    "    OUTPUT_FILE_NAME = f\"model_{idx}.txt\"\n",
    "    SKIP_MODEL = [] # Add any model to skip from 0 up to (FOLD_NUM-1)\n",
    "    # END OF CHANGEABLE PART\n",
    "    if idx in SKIP_MODEL:\n",
    "        continue\n",
    "    \n",
    "    model = YOLO(TRAINED_MODEL_PATH)\n",
    "    results = model.predict(\n",
    "        source  = CLASSIFIED_PATH,\n",
    "        save    = True,\n",
    "        imgsz   = 640,\n",
    "        device  = 0,\n",
    "        iou     = IOU,\n",
    "        conf    = CONF,\n",
    "        augment = True\n",
    "    )\n",
    "    output_file = open(OUTPUT_FILE_NAME, 'w')\n",
    "    for i in range(len(results)):\n",
    "        filename = results[i].path.split('/')[-1].split('.png')[0]\n",
    "        boxes = results[i].boxes\n",
    "        box_num = len(boxes.cls.tolist())\n",
    "        if box_num > 0:\n",
    "            for j in range(box_num):\n",
    "                label = int(boxes.cls[j].item())\n",
    "                conf = boxes.conf[j].item()\n",
    "                x1, y1, x2, y2 = boxes.xyxy[j].tolist()\n",
    "\n",
    "                line = f\"{filename} {label} {conf:.4f} {int(x1)} {int(y1)} {int(x2)} {int(y2)}\\n\"\n",
    "                output_file.write(line)\n",
    "\n",
    "    output_file.close()\n",
    "    \n",
    "    # FREE GARBAGE COLLECTION, TO AVOID Out-of-Memory\n",
    "    # DO NOT REMOVE\n",
    "    import torch, gc\n",
    "    del boxes, results, model\n",
    "    gc.collect()\n",
    "    torch.cuda.empty_cache()\n",
    "    # DO NOT REMOVE"
   ]
  },
